<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="Lab5AngeMerida"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lab5AngeMerida"
        tools:targetApi="31">
        <activity
            android:name=".ConcertInfoActivity"
            android:exported="false"
            android:label="ConcertInfoActivity"
            android:theme="@style/Theme.Lab5AngeMerida" />
        <activity
            android:name=".PlacesListActivity"
            android:exported="false"
            android:label="PlacesListActivity"
            android:theme="@style/Theme.Lab5AngeMerida" />
        <activity
            android:name=".ProfileActivity"
            android:exported="false"
            android:label="ProfileActivity"
            android:theme="@style/Theme.Lab5AngeMerida" />
        <activity
            android:name=".EventDetailActivity"
            android:exported="false"
            android:label="EventDetailActivity"
            android:theme="@style/Theme.Lab5AngeMerida" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Lab5AngeMerida"
            android:theme="@style/Theme.Lab5AngeMerida">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
